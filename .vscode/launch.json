{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "protocol": "inspector",
            "name": "Run Mocha",
            "program": "${workspaceRoot}/node_modules/mocha/bin/mocha",
            "args": [
                "--require",
                "espower-typescript/guess",
                "${workspaceRoot}/test/**/*.ts"
            ],
            "cwd": "${workspaceRoot}",
            "outFiles": []
        }
    ]
}
